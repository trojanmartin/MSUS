		<event type="assign">
			<id>3_assign</id>
			<actions phase="post">
				<action>
				suma: f.number_1,
				datum: f.text_0;
				
				
	def userCase = findCase({it.processIdentifier.eq("5f86b237f9ac3b272d6be569_User")})
	finishTask("3");

if(suma.value &lt;= userCase.dataSet["number_3"].value)
{	
	def konto = userCase.dataSet["number_3"].value - suma.value;

	def profil = findTask({it.caseId.eq(userCase.stringId) &amp; it.transitionId.eq("5")});
	
	setData(profil, [
			 "number_3": [
			 "value": konto,
			 "type" : "number"
			 ],
			])
	
	 Date date = new Date(); 
	
	change datum value {
		date.toString();
	}
	
    assignTask("61");	
}
else{
    def failure = findTask({ it.transitionId.eq("63")})
    assignTask(failure) 
}
</action>
			</actions>
		</event>
