					<behavior>required</behavior>
					<action trigger="set">
						suma: f.number_1,
						mesiac: f.this,
						text: f.text_0,
						predplatne: f.enumeration_0;
						
						
						def sluzba = findCase({it.processIdentifier.eq("5f86b237f9ac3b272d6be569_Služba")})																	
						
						def prices = sluzba.dataSet["subscriptionPrice"].options						
						def currentPrice;
						
						for (entry in prices) {
							if(entry.key.equals(predplatne.value.toString()))
							{
								currentPrice = entry.value as String
							}
						}					
						
						change suma value {
							mesiac.value * (currentPrice as Double)
						}						
					</action>
				</logic>
